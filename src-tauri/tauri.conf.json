{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Moonup",
  "version": "1.0.0",
  "identifier": "com.mooncloud.moonup",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Moonup",
        "width": 400,
        "height": 650,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {}
  },
  "bundle": {
    "active": true,
    "targets": ["msi", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Mooncloud"
  }
}