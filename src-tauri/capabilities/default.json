{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Standardberechtigungen f√ºr Moonup",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-ask",
    "fs:allow-read-text-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-create",
    "fs:allow-remove",
    "fs:allow-read-dir",
    "fs:scope-app-recursive",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled",
    {
      "identifier": "fs:scope",
      "allow": ["**"]
    }
  ]
}